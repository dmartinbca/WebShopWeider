{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,svg,webmanifest,xml,json}"
    ]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}